{
    "name": "msgphp/symfony-demo-app",
    "type": "project",
    "license": "MIT",
    "description": "A message driven Symfony demo application with basic user management, a REST/GraphQL API and OAuth/JWT authentication.",
    "require": {
        "api-platform/api-pack": "^1.1",
        "elasticsearch/elasticsearch": "^6.0",
        "hwi/oauth-bundle": "^0.6",
        "lexik/jwt-authentication-bundle": "^2.5",
        "msgphp/eav-bundle": "^0.6",
        "msgphp/user-bundle": "^0.6",
        "pascaldevink/shortuuid": "^2.1",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/httplug-bundle": "^1.11",
        "ramsey/uuid": "^3.8",
        "ramsey/uuid-doctrine": "^1.5",
        "sensio/framework-extra-bundle": "^5.2",
        "symfony/console": "^4.1",
        "symfony/flex": "^1.1",
        "symfony/form": "^4.1",
        "symfony/framework-bundle": "^4.1",
        "symfony/messenger": "^4.1",
        "symfony/monolog-bundle": "^3.3",
        "symfony/orm-pack": "^1.0",
        "symfony/security-bundle": "^4.1",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/translation": "^4.1",
        "symfony/validator": "^4.1",
        "webonyx/graphql-php": "^0.11"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "sroze/companienv": "^0.0.9",
        "symfony/debug-pack": "^1.0",
        "symfony/dotenv": "^4.1",
        "symfony/maker-bundle": "^1.6",
        "symfony/phpunit-bridge": "^4.1",
        "symfony/web-server-bundle": "^4.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "cache:clear": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01BTK0AY5K45QX57R6VCT18RPJ",
            "allow-contrib": true
        }
    }
}
