{
    "name": "msgphp/symfony-demo-app",
    "type": "project",
    "license": "MIT",
    "description": "A message driven Symfony demo application with basic user management, a REST/GraphQL API and OAuth/JWT authentication.",
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "api-platform/api-pack": "^1.1",
        "elasticsearch/elasticsearch": "^6.0",
        "hwi/oauth-bundle": "^0.6",
        "lexik/jwt-authentication-bundle": "^2.6",
        "msgphp/eav-bundle": "^0.8",
        "msgphp/user-bundle": "^0.8",
        "msgphp/user-eav": "^0.8",
        "pascaldevink/shortuuid": "^2.1",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/httplug-bundle": "^1.12",
        "ramsey/uuid": "^3.8",
        "ramsey/uuid-doctrine": "^1.5",
        "sensio/framework-extra-bundle": "^5.2",
        "symfony/console": "dev-master",
        "symfony/flex": "^1.1",
        "symfony/form": "dev-master",
        "symfony/framework-bundle": "dev-master",
        "symfony/messenger": "dev-master",
        "symfony/monolog-bundle": "^3.3",
        "symfony/orm-pack": "^1.0",
        "symfony/security-bundle": "dev-master",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/translation": "dev-master",
        "symfony/validator": "dev-master",
        "webonyx/graphql-php": "^0.12"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "sroze/companienv": "dev-master",
        "symfony/debug-pack": "^1.0",
        "symfony/dotenv": "dev-master",
        "symfony/maker-bundle": "^1.9",
        "symfony/phpunit-bridge": "dev-master",
        "symfony/web-server-bundle": "dev-master"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01BTK0AY5K45QX57R6VCT18RPJ",
            "allow-contrib": true,
            "require": "4.2.*"
        }
    }
}
